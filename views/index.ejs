<!doctype html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <link rel="stylesheet" href="/css/main.css">
        <title>fsechatroom</title>
    </head>
    <body>


        <div class="foo">

            <div class="soo">
                <h2 >FSE Chat Room</h2>
                <span id="status"></span>
            </div>

            <div id="viewmessage">
            </div>
            <div class="present">
                  <input id="colorStyle" type="color" placeHolder='#000' title="font color" />
                <textarea id="messageInput" ></textarea>
                <input id="sendBtn" type="button" value="SEND">
            </div>
        </div>

            <div id="loginfoo">
                <p id="info">Login.......</p>
                <div id="nickfoo">
                <input type="text" placeHolder="nickname" id="nicknameInput" />
                <input type="button" value="OK" id="loginBtn" />
            </div>
            </div>

        <script src="/js/fsechatroom.js"></script>
        <script src="/socket.io/socket.io.js"></script>
       
    </body>
</html>
